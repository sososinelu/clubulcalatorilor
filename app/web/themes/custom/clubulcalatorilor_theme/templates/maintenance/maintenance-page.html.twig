<div id="page-wrapper">
  <div id="page">
    <header id="site-header" role="banner">
      <div id="block-clubul-calatorilor-theme-branding" class="contextual-region block block-system block-system-branding-block">
        <div class="branding-name">
          <a href="/" title="Home" rel="home">
            <p>Welcome to the</p>
            <p>Clubul Calatorilor</p>
          </a>
        </div>
        <div class="branding-logo">
          <a href="/" title="Home" rel="home">
            <img src="/sites/default/files/logo.png" alt="Home">
          </a>
        </div>
      </div>
      <div class="section clearfix">
        {% if site_name or site_slogan %}
          <div id="name-and-slogan"{{ hide_site_name and hide_site_slogan ? ' class="visually-hidden"' }}>
            {% if site_slogan %}
              <div id="site-slogan"{{ hide_site_slogan ? ' class="visually-hidden"' }}>
                {{ site_slogan }}
              </div>
            {% endif %}
          </div>
        {% endif %}
      </div>
    </header>
    <div id="main-wrapper">
      <div id="main" class="clearfix">
        <main id="content" class="column" role="main">
          <section class="section">
            <a id="main-content"></a>
            {% if title %}
              <h1 class="title" id="page-title">{{ title }}</h1>
            {% endif %}
            {{ page.content }}
            {{ page.highlighted }}
          </section>
        </main>
      </div>
    </div>
    <footer id="site-footer" role="contentinfo">
      <section class="footer_bottom">
        <div class="">
          <div id="block-copyright-2" class="contextual-region block block-copyright-block">
            <p>Clubul Calatorilor Â© 2017</p>
          </div>

        </div>
      </section>
    </footer>
  </div>
</div>
