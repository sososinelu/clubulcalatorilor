{
    "name": "drupal-composer/roche-fphi",
    "description": "Clubul Calatorilor - Drupal 8 project with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/adminimal_theme": "^1.3",
        "drupal/backup_migrate": "^4.0",
        "drupal/block_exclude_pages": "^1.2",
        "drupal/core": "~8.6",
        "drupal/console": "^1.0.2",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/entity_reference_revisions": "^1.5",
        "drupal/eu_cookie_compliance": "^1.2",
        "drupal/facebook_pixel": "^1.0@alpha",
        "drupal/field_group": "^3.0@beta",
        "drupal/file_mdm": "^1.1",
        "drupal/google_analytics": "^2.3",
        "drupal/image_effects": "^2.1",
        "drupal/libraries": "^3.0@alpha",
        "drupal/metatag": "^1.5",
        "drupal/neato": "^1.8",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.3",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/simple_sitemap": "^2.12",
        "drupal/sitemap": "^1.4",
        "drupal/token": "^1.3",
        "drupal/twig_tweak": "^2.1",
        "drupal/webform": "^5.0@RC",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "wikimedia/composer-merge-plugin": "^1.4",
        "sendgrid/sendgrid": "~7"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0.30"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
